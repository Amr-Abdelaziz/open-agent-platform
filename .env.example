# ====================================
# Environment Variables Template
# ====================================
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to version control

# ====================================
# Supabase Configuration (Required)
# ====================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# ====================================
# API Configuration (Required)
# ====================================
# Base API URL for LangGraph/LangSmith
NEXT_PUBLIC_BASE_API_URL=http://localhost:8123

# RAG API URL for RAG operations
NEXT_PUBLIC_RAG_API_URL=http://gorbit:8080

# ====================================
# Deployments Configuration (Required)
# ====================================
# JSON array of deployment configurations
# Each deployment should have: id, name, url, auth_type, and optional api_key
NEXT_PUBLIC_DEPLOYMENTS=[{"id":"default","name":"Default","url":"http://localhost:8123","auth_type":"local_auth"}]

# ====================================
# MCP Server Configuration (Optional)
# ====================================
# MCP (Model Context Protocol) server URL
NEXT_PUBLIC_MCP_SERVER_URL=

# Whether MCP server requires authentication
NEXT_PUBLIC_MCP_AUTH_REQUIRED=false

# ====================================
# Authentication Configuration (Optional)
# ====================================
# Use LangSmith authentication for proxy routes
NEXT_PUBLIC_USE_LANGSMITH_AUTH=false

# Disable Google OAuth authentication
NEXT_PUBLIC_GOOGLE_AUTH_DISABLED=false

# ====================================
# Application Configuration (Optional)
# ====================================
# Run app in demo mode
NEXT_PUBLIC_DEMO_APP=false

# ====================================
# Docker Configuration (Optional)
# ====================================
# Port for web application
WEB_PORT=3000

# Port for documentation site
DOCS_PORT=3001
